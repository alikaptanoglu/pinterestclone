<!DOCTYPE html>
<html>
  <head>
    <title>Kimterest</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <script type="text/javascript">
        window.splashURL = "<%= image_url('splash.png') %>";
        window.logoURL = "<%= image_url('logo.png') %>";
        window.feedlogoURL = "<%= image_url('header_logo.png') %>";
        window.defaultpfpURL = "<%= image_url('defaultpfp.png') %>";
        window.plusURL = "<%= image_url('plus.svg') %>";
        window.pencilURL = "<%= image_url('pencil.svg') %>";
        window.uploadURL = "<%= image_url('upload.svg') %>";
        window.menuURL = "<%= image_url('ellipsis.png') %>";
        window.editURL = "<%= image_url('edit.png') %>";
        window.bgURL = "<%= image_url('boardbg.png') %>";
        window.backURL = "<%= image_url('back.png') %>";

      

    </script>
    <%= favicon_link_tag asset_path('header_logo.png') %>
  </head>

  <body>
    
    <%= yield %>
  </body>
</html>
